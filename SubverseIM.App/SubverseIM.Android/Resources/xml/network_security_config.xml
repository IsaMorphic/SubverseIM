<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
	<domain-config>
		<domain includeSubdomains="false">subverse.network</domain>
		<trust-anchors>
			<certificates src="@raw/trusted_root"/>
		</trust-anchors>
	</domain-config>
	<debug-overrides>
		<trust-anchors>
			<certificates src="user"/>
		</trust-anchors>
	</debug-overrides>
</network-security-config>